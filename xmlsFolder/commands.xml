<?xml version="1.0" encoding="ISO-8859-1"?>
<!--Make sure you are using the place holders right, first update the name of the place holder, then use it-->
<!--Name of command must define in constants-->
<Commands>
	<PlaceHolders>
		<PlaceHolder id="Version" name="VERSION" type="string" />
		<PlaceHolder id="BB" name="BB" type="string" />
		<PlaceHolder id="Version_" name="VERSION_" type="string" />
		<PlaceHolder id="Tlg_domain" name="TLGDOMAIN" type="string" />
	</PlaceHolders>

	<UnixCommands>
			<command id="1" name="Clear Backend Generator" 
command="find ~/bb/TlgServer/{VERSION_}/amdocs/TlgServer/backend/datalayers_v36/ -type f -exec rm -f '{}' \; ;find ~/bb/TlgServer/{VERSION_}/amdocs/TlgServer/backend/dl_interfaces/ -type f -exec rm -f '{}' \; ;find ~/bb/TlgServer/{VERSION_}/amdocs/TlgServer/backend/datatypes/ -type f -exec rm -f '{}' \; ;end_activity;"
type="string"/>
<command id="2" name="Run build in CC" 
			command = "/ibmshome/ibms/ibms/mb_ccird/bin/request_API_build -v {VERSION} -p sbmsIrd{VERSION} \n"
type="string"/>
<command id="3" name="Check CC log"
			command = "cd /ibmshome/ibms/ibms/ccird/log.sbmsIrd{VERSION}/log.{BB};grep -l 'BUILD SUCCESSFUL' `ls -rt |tail -2` ;echo $? \n"
type="string"/>
<command id="4" name="Run local build"
			command = "set_proj -p sbmsIrd{VERSION} -b {BB} ; rm -rf ~/weblogic/tlg_domain/classes/amdocs/;cd ~/weblogic/{TLGDOMAIN}; Test_LOCAL_BUILD1;@"
type="string"/>
<command id="5" name="RefreshLocal"
			command = "cd ~/weblogic/{TLGDOMAIN}; refreshLocal.sh;"
type="string"/>
<command id="6" name="Refresh TlgServer"
			command = "cd ~/weblogic/{TLGDOMAIN}; refreshTlgServer.sh;"
type="string"/>
<command id="7" name="Restart TlgServer"
			command = "cd ~/weblogic/{TLGDOMAIN}; stopTlgServer.sh; startTlgServer.sh; tail -f logs/weblogic.*;"
type="string"/>
<command id="8" name="Quick build"
			command = "set_proj -p sbmsIrd{VERSION} -b {BB} ;rm -rf ~/ weblogic/{TLGDOMAIN}/classes/amdocs ;cp -rf ~/bb/{BB}/{VERSION_}/bin/ ~/weblogic/{TLGDOMAIN}/classes/amdocs ; "
type="string"/>
<command id="9" name="Clear Classes"
			command = "rm –rf ~/weblogic/{TLGDOMAIN}/classes/amdocs \n"
type="string"/>
<command id="10" name="Check local log"
			command = "write command that checks the local build log \n"
type="string"/>
<command id="11" name="Create new BB"
			command = "set_proj -p sbmsIrd{VERSION} -b {BB}; source /usr/local/site-init-files/site.cshrc; source /etc/.login; source ~/.login; work_proj sbmsIrd{VERSION}; work_bb sbmsIrd{VERSION} {BB}; set_proj -p sbmsIrd{VERSION} -b {BB} \n"
type="string"/>
<command id="12" name="Change project"
			command = "set_proj -p sbmsIrd{VERSION} -b {BB}"
type="string"/>

	</UnixCommands>
</Commands>